programa {
  funcao inicio() {
    cadeia nome
    inteiro nascimento
    real salario
    escreva("Nome do funcionário: ")
    leia(nome)
    escreva("Ano de nascimento: ")
    leia(nascimento)
    escreva("Salário: R$ ")
    leia(salario)
    escreva("\n----------FICHA FUNCIONAL-----------\n")
    escreva("NOME: " + nome + "\n")
    escreva("NASCIMENTO: em " + nascimento + "\n")
    escreva("SALÁRIO de R$" + salario + "\n")
    escreva("--------------------------------------")
  }
}
